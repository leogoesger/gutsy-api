{
  "name": "gutsy_api",
  "args": ["--toto=heya coco", "-d", "1"],
  "script": "/gutsy-api/src/index.js",
  "node_args": ["--harmony", " --max-stack-size=102400000"],
  "log_date_format": "YYYY-MM-DD HH:mm Z",
  "watch": false,
  "ignore_watch": ["[\\/\\\\]\\./", "node_modules"],
  "merge_logs": true,
  "exec_interpreter": "./node_modules/.bin/babel-node",
  "exec_mode": "fork",
  "autorestart": true,
  "env_production": {
    "NODE_ENV": "production"
  },
  "env_staging": {
    "NODE_ENV": "staging",
    "TEST": true
  }
}
