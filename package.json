{
  "name": "gutsy-api",
  "version": "1.0.0",
  "description":
    "A climbing guide contains only accurate and up-to-date information!",
  "main": "index.js",
  "author": "Leo Qiu",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">= 6.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "start": "nodemon ./src/index.js --exec babel-node",
    "production":
      "NODE_ENV=production nodemon ./src/index.js --exec babel-node",
    "mocha":
      "./node_modules/.bin/_mocha test/service --exit --require babel-register --require babel-polyfill",
    "test": "NODE_ENV=test yarn run test-prepare ; NODE_ENV=test npm run mocha",
    "test-cover":
      "NODE_ENV=test yarn run test-prepare ; NODE_ENV=test nyc --reporter=text --reporter=lcov npm run mocha",
    "test-coverage": "nyc report --reporter=lcov",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "test-prepare": "sequelize db:migrate:undo:all ; yarn run migrate ",
    "dev-prepare":
      "sequelize db:migrate:undo:all ; yarn run migrate ; yarn run seed",
    "lint":
      "node_modules/.bin/eslint --fix --ext \".js\" --ignore-pattern node_modules --quiet .",
    "prettier":
      "prettier --single-quote --bracket-spacing false --trailing-comma es5 --write '{src,__{tests,mocks}__}/**/*.js'"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-plugin-prettier": "^2.0.1",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "factory-girl": "^5.0.1",
    "jsonwebtoken": "^8.1.0",
    "mocha": "^4.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.3.0",
    "pg": "^6.4.1",
    "pg-hstore": "^2.3.2",
    "prettier": "1.7.4",
    "sequelize": "^4.15.0",
    "sequelize-cli": "^3.0.0",
    "lodash": "4.17.4"
  },
  "devDependencies": {}
}
