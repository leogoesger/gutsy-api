{
  "name": "rockyard-api",
  "version": "1.0.0",
  "description": "Climbing community performance data tracker!",
  "main": "index.js",
  "author": "Leo Qiu",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "nodemon ./src/index.js --exec babel-node",
    "test":
      "NODE_ENV=test npm run prepare ; NODE_ENV=test npm run migrate ; NODE_ENV=test mocha test/service --require babel-register --require babel-polyfill",
    "test-watch": "nodemon --exec 'yarn test'",
    "migrate": "sequelize db:migrate",
    "prepare": "sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "factory-girl": "^5.0.1",
    "jsonwebtoken": "^8.1.0",
    "mocha": "^3.3.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.12.1",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.15.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-plugin-prettier": "^2.0.1",
    "prettier": "1.7.4",
    "sequelize-cli": "^3.0.0"
  }
}
